{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "npm-run-all build:clean build:compile",
    "build:clean": "rimraf dist",
    "build:compile": "tsc -p tsconfig.build.json",
    "database:dev:clean": "rimraf src/db/*.sqlite3 dist/db/*.sqlite3",
    "start": "node dist/index.js",
    "start:dev": "npm-run-all database:dev:clean start:dev:run",
    "start:dev:run": "ts-node src/index.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write  'src/**/*.ts'",
    "test": "npm-run-all test:unit test:int test:e2e",
    "test:unit": "jest --config jest.config.js --collect-coverage --verbose",
    "test:int": "jest --config jest.config.int.js --collect-coverage --verbose",
    "test:e2e": "npm-run-all -c database:dev:clean test:e2e:setup test:e2e:run test:e2e:teardown",
    "test:e2e:setup": "echo 'Starting server...' && node dist/index.js & sleep 3 && echo 'Server started.'",
    "test:e2e:teardown": "killall -q -9 'node dist/index.js' || true",
    "test:e2e:run": "jest --config jest.config.e2e.js --runInBand --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "jet-logger": "^1.3.1",
    "lodash": "^4.17.21",
    "middleware-async": "^1.4.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.3",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typestrong/ts-mockito": "^2.6.6",
    "axios": "^1.6.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
